[color]
ui = true
[core]
editor = vim
[diff]
tool = vscode
[difftool "vscode"]
cmd = code --wait --diff $LOCAL $REMOTE
[merge]
tool = vscode
ff = false
[mergetool "vscode"]
cmd = code --wait $MERGED
[init]
defaultBranch = main
[pull]
ff = only
